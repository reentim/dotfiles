#!/usr/bin/env ruby --disable-gems

themes = [
  'Solarized Light',
  'Solarized Dark',
  'Black',
]

fonts = [
  'Anon',
  'Fira Code',
  'Inconsolata',
  'Input Mono Condensed',
  'Input Mono Narrow',
  'Input Mono',
  'Plex',
  'Pragmata',
  'SF',
  'Source',
]

puts themes.map { |theme| fonts.map { |font| "#{theme} - #{font}" } }
