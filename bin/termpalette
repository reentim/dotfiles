def putscolor(num, hsl, i)
  print format "%03d \e[48;5;#{num}m       \e[0m", num
  puts
end

sorted_colors = termcolors
  .sort_by { |a| [a[:hsl][0], a[:hsl][2] * -1, a[:hsl][1]] }

sorted_colors.map.with_index do |color, i|
  putscolor(
    color[:id],
    color[:hsl],
    i,
  )
end
