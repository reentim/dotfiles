[user]
  name = Timothy Reen
  email = reentim@gmail.com
[core]
  editor = vim
  excludesfile = "~/.gitignore-global"
[color]
  ui = auto
  status = auto
  branch = auto
  interactive = auto
  diff = auto
[alias]
  l       = log --graph --pretty=format:\"%C(yellow)%h%C(green) %ar%C(blue) %an%C(red)%d%C(reset) %s\"
  graph   = log --graph --pretty=format:\"%C(yellow)%h%C(green) %ar%C(blue) %an%C(red)%d%C(reset) %s\"
  g       = log --graph --pretty=format:\"%C(yellow)%h%C(green) %ar%C(blue) %an%C(red)%d%C(reset) %s\"
  st      = status
  ci      = commit
  cim     = commit -m
  co      = checkout
  di      = diff
  ds      = diff --staged
  aa      = add -A
  diw     = diff --color-words
  dsw     = diff --staged --color-words
  pf      = pull --ff-only
  amend   = commit --amend
  unstage = reset HEAD
  ff      = merge --ff-only
[merge]
  tool = vimdiff
  defaultToUpstream = true
[push]
  default = tracking
